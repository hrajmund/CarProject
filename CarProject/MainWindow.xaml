<Window x:Class="CarProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarProject"
        mc:Ignorable="d"
        Title="Endless Runner" Height="450" Width="800"
        Icon="images/coin.png" Background="#1E221C">
    <Grid Background="#1E221C">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Grid.Row="1" Margin="115,50,115,44" FontSize="30" Foreground="#E6AF2E" Background="#403233" VerticalContentAlignment="Center"  HorizontalContentAlignment="Center" Content="New Game" x:Name="NewGame" Click="New_" RenderTransformOrigin="0.5,0.5" FontFamily="Barlow Condensed Black" >
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="5"/>
                </Style>
                
            </Button.Resources>
            
        </Button>
    </Grid>
</Window>
