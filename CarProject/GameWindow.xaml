        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarProject"
        mc:Ignorable="d"
        Title="GameWindow" Height="720" Width="1280">
    <Canvas x:Name="myCanvas"  Focusable="True" KeyDown="_Keydown" KeyUp="_Keyup"  Margin="-2,-393,-8,-21">


        <Rectangle Name="car1" Canvas.Top="924" Canvas.Left="607" Height="128" Width="64">
            <Rectangle.Fill>
                <ImageBrush ImageSource="_cars\car1.png"></ImageBrush>
            </Rectangle.Fill>
        </Rectangle>
        <Rectangle x:Name="Rectan" Fill="Black" Tag="Collectibles" Canvas.Top="234" Canvas.Left="686" Width="32" Height="32" RenderTransformOrigin="1.156,-3.406">

        </Rectangle>
        <Rectangle x:Name="Ettan" Fill="Black" Tag="Collectibles" Canvas.Top="302" Canvas.Left="449" Width="32" Height="32" RenderTransformOrigin="1.156,-3.406">

        </Rectangle>
        <Rectangle x:Name="fasz" Fill="Black" Tag="Collectibles" Canvas.Top="434" Canvas.Left="639" Width="32" Height="32" RenderTransformOrigin="1.156,-3.406">

        </Rectangle>
        <TextBox  x:Name="myScore"  Canvas.Left="28" Canvas.Top="434"/>
    </Canvas>
</Window>
